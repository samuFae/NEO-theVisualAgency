{
  "name": "neo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f ./.env.local craco start",
    "build": "env-cmd -f ./.env.prod craco build",
    "lint": "eslint ./src/**/*.tsx ./src/**/*.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "namespace": "NEO",
        "displayName": true,
        "fileName": true
      }
    ]
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "env-cmd": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-scripts": "5.0.1",
    "redux-persist": "^6.0.0",
    "rxjs": "^7.8.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0",
    "@types/babel__core": "^7.20.0",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/styled-components": "^5.1.26",
    "craco-alias": "^3.0.1",
    "prettier": "^2.7.1",
    "typescript": "^4.9.3"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}